  "tray": {
    "icon-size": 22,
    "spacing": 16
  },
  "pulseaudio": {
    "format": "{icon} {volume}% |",
    "tooltip": true,
    "format-muted": " üîá |",
    "format-bluetooth": " ÔÄ• ",
    "on-click": "pamixer -t",
    "on-click-middle": "com.mastermindzh.tidal-hifi",
    "on-click-right": "pavucontrol-qt",
    "on-scroll-up": "pamixer -i 5",
    "on-scroll-down": "pamixer -d 5",
    "scroll-step": 5,
    "format-icons": {
      "headphone": " ÔÄ• ",
      "hands-free": " ÔÄ• ",
      "headset": " ÔÄ• ",
      "phone": " ÔÇï ",
      "portable": " ÔÇï ",
      "car": " ÔÜπ ",
      "default": [" ÔÄ¶ ", " ÔÄß ", " ÔÄ® "],
      "hifi": " ùÑ¢ "
    }
  },
  "pulseaudio#microphone": {
    "format": "|{format_source}",
    "format-source": " ÔÑ∞ ",
    "format-source-muted": " ÔÑ± ",
    "scroll-step": 5,
    "on-click": "pamixer --default-source -t",
    "on-click-right": "pavucontrol-qt"
  },
  "custom/swaync": {
    "format": " ÔÉ≥ ",
    "exec": "swaync -swb",
    "on-click": "swaync-client -t"
