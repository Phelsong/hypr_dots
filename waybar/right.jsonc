  },
  "group/utils": {
    "orientation": "horizontal",
    "modules": ["custom/lock", "custom/power", "custom/reboot"],
    "drawer": {
      "transition-duration": 500,
      "children-class": "not-power",
      "transition-left-to-right": false
    }
  },
  "custom/lock": {
    "format": "Û∞çÅ",
    "tooltip": false,
    "on-click": "hyprlock"
  },
  "custom/reboot": {
    "format": "Û∞úâ",
    "tooltip": false,
    "on-click": "reboot"
  },
  "custom/power": {
    "format": "ÔÄë",
    "tooltip": false,
    "on-click": "shutdown now"
  }
}

